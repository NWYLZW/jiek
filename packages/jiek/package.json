{
  "name": "jiek",
  "version": "0.4.7",
  "description": "YiJie's personal kits.",
  "bin": {
    "jiek": "bin/jiek.js",
    "jk": "bin/jiek.js"
  },
  "jiek": {
    "noBrowser": true
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts",
    "./rollup": "./src/rollup/index.ts",
    "./rollup.v2": "./src/rollup/v2.ts"
  },
  "imports": {
    "#~/*": "./src/*"
  },
  "dependencies": {
    "@jiek/pkger": "workspace:^",
    "@jiek/rollup-plugin-dts": "^6.1.1",
    "@jiek/utils": "workspace:^",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.16",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "detect-indent": "^6.1.0",
    "execa": "9.3.1",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.2.1",
    "micromatch": "^4.0.5",
    "rollup": "^4.1.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "@pnpm/filter-workspace-packages": "^7.2.13",
    "esbuild-register": "^3.5.0",
    "postcss": "^8.4.47",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "devDependencies": {
    "@npm/types": "^1.0.2",
    "@pnpm/filter-workspace-packages": "^7.2.13",
    "@pnpm/workspace.pkgs-graph": "^2.0.15",
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/micromatch": "^4.0.6",
    "esbuild-register": "^3.5.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.47",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs",
        "inner-src": "./src/index.ts"
      },
      "./cli": {
        "types": "./dist/cli.d.ts",
        "import": "./dist/cli.esm.js",
        "require": "./dist/cli.cjs",
        "inner-src": "./src/cli.ts"
      },
      "./rollup": {
        "types": "./dist/rollup.d.ts",
        "import": "./dist/rollup.esm.js",
        "require": "./dist/rollup.cjs",
        "inner-src": "./src/rollup/index.ts"
      }
    },
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.esm.js",
    "typesVersions": {
      "<5.0": {
        "*": [
          "*",
          "./dist/*",
          "./dist/*/index.esm.d.ts"
        ]
      }
    }
  }
}
